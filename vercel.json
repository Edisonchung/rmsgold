{
  "version": 2,
  "buildCommand": "echo 'Build files already exist'",
  "outputDirectory": "build/web",
  "framework": null,
  "routes": [
    {
      "src": "/(.*\\.(js|css|woff2?|png|jpe?g|gif|svg|ico|json|wasm))",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
